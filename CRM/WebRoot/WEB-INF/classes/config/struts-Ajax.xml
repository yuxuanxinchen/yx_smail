<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<package name="ajax" extends="struts-default" namespace="/ajax">

		<action name="customerInfoAjax" class="customerInfoAction">
			<result>/WEB-INF/form/addcInfoAjax.jsp</result>
		</action>

		<action name="updateComtomerInfoBefore" class="updateComtomerInfoBeforeAction">
			<result>/WEB-INF/form/UpdatacInfoAjax.jsp</result>
		</action>

		<action name="customerInfoWindow" method="ajaxWindow"
			class="customerInfoAllAction">
			<result>/WEB-INF/form/ShowContactsInfo.jsp</result>
		</action>

		<action name="contactsInfoAjax" class="contactsInfoAjax">
			<result type="chain">contactsInfodata</result>
		</action>
		
		<action name="customerChartp" method="chartP" class="customerInfoAction">
			<result>/WEB-INF/form/complainCharts.jsp</result>
		</action>
		
     


	</package>
</struts>    
