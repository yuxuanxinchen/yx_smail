	    <bean id="contactsInfoActionAllData" class=" com.yuxuan.action.ContactsInfoAction">
	     		<property name="contactsService" ref="contactsService"></property>
	    </bean>	 
	    
	      <!-- 菜单-->	  
	    <bean id="menuInfoAction" class="com.yuxuan.action.MenuInfoAction">
