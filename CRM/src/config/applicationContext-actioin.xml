	        <property name="sjzdxxService" ref="sjzdxxService"></property>
	    </bean>	    
	    
	    
	    <!-- 数据字典分类信息-->	  
	    <bean id="sjzdxxAction" class="com.yuxuan.action.SjzdxxAction">
	        <property name="sjzdflService" ref="sjzdflService"></property>
	        <property name="sjzdxxService" ref="sjzdxxService"></property>
	    </bean>	    
	    
	    <!-- 更新数据字典分类信息-->	  
	    <bean id="updateSjzdxxAction" class="com.yuxuan.action.UpdateSjzdxxAction">
