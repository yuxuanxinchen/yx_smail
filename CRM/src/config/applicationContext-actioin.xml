	       		<property name="contactsService" ref="contactsService"></property>
	       		<property name="customerInfoService" ref="customerInfoService"></property>
	       		<property name="messionService" ref="messionService"></property>
	    </bean>	 
	    	      
	     <bean id="scheduleAction" class=" com.yuxuan.action.ScheduleAction">
	       		<property name="scheduleService" ref="scheduleService"></property>
	    </bean>	 
	    
	        <bean id="updataScheduleAction" class="com.yuxuan.action.UpdataScheduleAction">
	       		<property name="scheduleService" ref="scheduleService"></property>
	    </bean>	 
	   
	   	    
	        <bean id="projectManagerAction" class="com.yuxuan.action.ProjectManagerAction">
	       		<property name="projectService" ref="projectService"></property>
	    </bean>	 
	   
	   
	   
	  
